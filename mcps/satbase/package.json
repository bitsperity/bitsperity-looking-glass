{
  "name": "satbase-mcp",
  "version": "1.0.0",
  "description": "Model Context Protocol server for Satbase data access",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "dev:stdio": "tsx src/index-stdio.ts",
    "dev:http": "tsx src/index-http.ts",
    "build": "vite build",
    "start": "node dist/index.js",
    "start:stdio": "node dist/index-stdio.js",
    "start:http": "node dist/index-http.js",
    "test": "vitest"
  },
  "keywords": [
    "mcp",
    "satbase",
    "news",
    "macro",
    "prices",
    "financial-data"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "express": "^4.19.0",
    "pino": "^9.0.0",
    "pino-pretty": "^13.1.2",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vitest": "^2.1.0"
  }
}
